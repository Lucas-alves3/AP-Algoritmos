#include <stdio.h>

void desenhar_asteriscos(int num) {
    int i;
    
    for(i = 0; i < num; i++) {
        printf("*");
    }
    
    printf("\n");
}

int main() {
    int numero;
    
    printf("Digite um numero inteiro:");
    scanf("%d", &numero);
    
    desenhar_asteriscos(numero);
    
    return 0;
}
